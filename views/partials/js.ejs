<script>
  /*
  * Toggle menu 
  */
  const menuToggleBtn = document.getElementById('menu-toggle')

  menuToggleBtn.addEventListener('click', function () {
    $("#wrapper").toggleClass("toggled");

    if (document.getElementById("toggle-btn").className.match(/(?:^|\s)fa fa-chevron-left(?!\S)/)) {
        document.getElementById("toggle-btn").className = "fa fa-chevron-right";
    } else {
        document.getElementById("toggle-btn").className = "fa fa-chevron-left";
    }
  })
</script>